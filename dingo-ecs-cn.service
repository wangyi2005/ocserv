[Unit]
Description=Dingo EDNS - DNS over HTTPS
Wants=network.target

[Service]
Type=simple
ExecStart=/usr/bin/dingo -port 5353 -gdns:server 8.8.8.8 -gdns:sni dns.google -gdns:edns 61.177.77.0
ExecReload=/bin/kill -HUP $MAINPID
Restart=on-abort
StandardOutput=null

[Install]
WantedBy=multi-user.target
